<p id="notice"><%= notice %></p>
<%= render partial: "links/link", locals: {link: @link} %>
<%= button_to 'Add Comment', new_comment_path, :class => "button mt-10"%>
<% @link.comments.each do |comment| %>
    <div class="comments">
      </br>
      <%= comment.comment %></br>
      said by <%= comment.account_id %> at <%= comment.created_at %></br>
      </br>
    </div>
<% end %>
<!-- end of if signed_in -->
